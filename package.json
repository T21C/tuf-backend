    "cors": "^2.8.5",
    "cron": "^4.1.3",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.37.103",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "form-data": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "mailersend": "^2.3.0",
    "mongoose": "^8.8.0",
