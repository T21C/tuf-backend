{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "build-clear": "rimraf dist tsconfig.tsbuildinfo",
    "build-tsc": "tsc --project tsconfig.json --noEmit false",
    "build-copy-html": "node dist/scripts/copyHtml.js",
    "build": "npm run build-clear && npm run build-tsc && npm run build-copy-html",
    "dev": "cross-env NODE_ENV=development tsx watch --inspect --max-old-space-size=512 src/app.ts",
    "dev:serve": "cross-env NODE_ENV=development node dist/app.js",
    "staging": "cross-env NODE_ENV=staging node --max-old-space-size=512 --expose-gc dist/app.js",
    "production": "cross-env NODE_ENV=production node --no-deprecation dist/app.js",
    "debug": "cross-env NODE_ENV=production MODE=debug node --inspect --max-old-space-size=2048 --expose-gc dist/app.js",
    "stress-test": "cross-env NODE_ENV=development tsx watch --inspect --max-old-space-size=512 src/scripts/puppeteer-stress-test.ts",
    "lint": "gts lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "prepare": "npm run build",
    "recalculate-scores": "cross-env tsx src/scripts/recalculateScores.ts",
    "test-email": "cross-env tsx src/scripts/testEmail.ts",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:create": "sequelize-cli migration:create --name",
    "health": "cross-env tsx src/healthService/startHealthService.ts",
    "health:prod": "cross-env NODE_ENV=production tsx src/healthService/startHealthService.ts",
    "cdn": "cross-env tsx watch src/cdnService/app.ts",
    "migrate-level-zips": "cross-env tsx src/scripts/migrateLevelZips.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.1",
    "@google-cloud/recaptcha-enterprise": "^5.13.0",
    "@types/adm-zip": "^0.5.7",
    "@types/cron": "^2.4.3",
    "@types/multer": "^1.4.12",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "adm-zip": "^0.5.16",
    "adofai-lib": "^1.0.7",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cron": "^4.1.3",
    "cross-env": "^7.0.3",
    "discord-api-types": "^0.37.103",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "form-data": "^4.0.1",
    "json5": "^2.2.3",
    "jsonwebtoken": "^9.0.2",
    "mailersend": "^2.3.0",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.11.5",
    "node-disk-info": "^1.3.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "object-hash": "^3.0.0",
    "puppeteer": "^24.7.1",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "sharp": "^0.33.5",
    "socket": "^0.14.27",
    "socket.io": "^4.8.1",
    "twemoji": "^14.0.2",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/nodemailer": "^6.4.17",
    "@types/object-hash": "^3.0.6",
    "@types/shuffle-seed": "^1.1.3",
    "gts": "^6.0.2",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "~5.1.6"
  }
}
