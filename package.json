{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "build-clear": "rimraf dist tsconfig.tsbuildinfo",
    "build-tsc": "tsc --project tsconfig.json --noEmit false",
    "build": "npm run build-clear & npm run build-tsc",
    "dev": "cross-env NODE_ENV=development tsx watch src/app.ts",
    "staging": "cross-env NODE_ENV=staging node dist/app.js",
    "production": "cross-env NODE_ENV=production node dist/app.js",
    "lint": "gts lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "prepare": "npm run build",
    "recalculate-scores": "cross-env tsx src/scripts/recalculateScores.ts",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:create": "sequelize-cli migration:create --name"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
