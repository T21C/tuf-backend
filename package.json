    "@types/cron": "^2.4.3",
    "@types/multer": "^1.4.12",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^10.0.0",
    "adm-zip": "^0.5.16",
    "ansi-colors": "^4.1.3",
    "axios": "^1.7.7",
